<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
@if (TempData["success"] != null)
{
	<script>
		Swal.fire({
			position: 'top-end',
			icon: 'success',
			title: '@TempData["success"]',
			showConfirmButton: false,
			timer: 3000,
			timerProgressBar:true
		})
	</script>
}
@if (TempData["error"] != null)
{
	<script>
		Swal.fire({
			position: 'top-end',
			icon: 'error',
			title: '@TempData["error"]',
			showConfirmButton: false,
			timer: 3000,
			timerProgressBar:true
		})
	</script>
}